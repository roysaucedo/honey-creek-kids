    "image_text_hero_8T74VY": {
      "type": "image-text-hero",
      "settings": {
        "h1": "Better Kids Make A Better World",
        "p": "<p>HoneyCreek Kids aims to positively influence families by creating life-changing products and materials that inspire.<\/p>",
        "subtext": "Over 1,000,000+ copies sold",
        "button_text": "Explore Our Collections",
        "button_link": "shopify:\/\/collections\/all",
        "image": "shopify:\/\/shop_images\/Hero-Img-HoneyCreek_2.png",
        "absolute_image": "shopify:\/\/shop_images\/image_5_5.png"
      }
    },
    "section_reviews_slider_PDNUCL": {
      "type": "section-reviews-slider",
      "blocks": {
        "slide_H79nAt": {
          "type": "Slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_24_1.png",
            "rich_text": "<p>“A few of the students were crying because they felt so happy because they were touched by the music and the message. <span style=\"text-decoration:underline\"><strong>This is exactly what our school needed at this time.<\/strong><\/span>”<\/p><p><strong>Denis C. - Principal<\/strong><\/p>"
          }
        },
        "slide_wU8gdt": {
          "type": "Slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_24_1.png",
            "rich_text": "<p>\"I really want to thank you for the character book and the CD. It was fabulous. Also the vocabulary like honesty, kindness, [and] respect.\"<\/p><p><strong>Chaysity E. - Elementary Student<\/strong><\/p>"
          }
        },
        "slide_ndXFK3": {
          "type": "Slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_24_1.png",
            "rich_text": "<p>\"I sing the songs every day. And I also read the book. I loved everything.\"<\/p><p><strong>Sidney W. - Elementary Student<\/strong><\/p>"
          }
        }
      },
      "block_order": [
        "slide_H79nAt",
        "slide_wU8gdt",
        "slide_ndXFK3"
      ],
      "settings": {
        "h2": "Hear From Happy Customers",
        "h3": ""
      }
    },
